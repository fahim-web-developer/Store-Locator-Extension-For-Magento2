<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="style-src">
            <values>
                <value id="fonts_googleapis_style" type="host">fonts.googleapis.com</value>
            </values>
        </policy>
        <policy id="font-src">
            <values>
                <value id="fonts_gstatic" type="host">fonts.gstatic.com</value>

            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="maps_gstatics_img" type="host">maps.gstatic.com</value>
                <value id="maps_googleapis_img" type="host">maps.googleapis.com</value>
                <value id="cdn_rawgit_img" type="host">cdn.rawgit.com/googlemaps/</value>
                <value id="cdn_jsdelivr_img" type="host">cdn.jsdelivr.net/gh/googlemaps/</value>
            </values>
        </policy>
        <policy id="script-src">
            <values>
                <value id="maps_googleapis_script" type="host">maps.googleapis.com</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="maps_googleapis_src" type="host">maps.googleapis.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>